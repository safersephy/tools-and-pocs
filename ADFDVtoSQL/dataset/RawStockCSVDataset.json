{
	"name": "RawStockCSVDataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"debtorcode": {
				"type": "string",
				"defaultValue": "1337"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@{utcnow('yyyy-MM-dd')}stock.csv",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().debtorcode",
					"type": "Expression"
				},
				"fileSystem": "csvexport"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}